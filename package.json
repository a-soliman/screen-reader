{
  "name": "basics",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "env-cmd ./config/test.env jest --watch --runInBand",
    "start": "node dist/index.js",
    "heroku-postbuild": "babel src --out-dir dist --copy-files",
    "dev": "env-cmd ./config/dev.env nodemon src/index.js --ext js,graphql --exec babel-node",
    "get-schema": "graphql get-schema -p prisma --dotenv config/dev.env",
    "lint": "npx eslint src test *.js",
    "lint:changed": "git diff --diff-filter=ACM --name-only -r master | grep '\\.js$' > .jsfiles || true && [ ! -s .jsfiles ] || npx eslint --no-ignore $(<.jsfiles)",
    "prettier:changed": "git diff --diff-filter=ACM --name-only -r master | grep '\\.js$' > .jsfiles || true && [ ! -s .jsfiles ] || npx prettier $(<.jsfiles) --write",
    "pretest": "eslint --ignore-path .gitignore ."
  },
  "jest": {
    "globalSetup": "./tests/jest/globalSetup.js",
    "globalTeardown": "./tests/jest/globalTeardown.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "apollo-boost": "^0.1.14",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-node": "0.0.1-security",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "cross-fetch": "^2.2.2",
    "env-cmd": "^8.0.2",
    "generate-password": "^1.4.2",
    "graphql": "^14.0.2",
    "graphql-cli": "^2.16.4",
    "graphql-import": "^0.7.1",
    "graphql-yoga": "^1.16.7",
    "jsonwebtoken": "^8.3.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "prisma-binding": "^2.1.1"
  },
  "devDependencies": {
    "jest": "^23.5.0",
    "eslint": "^6.6.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.16.0"
  }
}
